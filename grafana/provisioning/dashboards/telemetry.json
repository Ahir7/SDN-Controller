{
  "dashboard": {
    "title": "SDN Telemetry Dashboard",
    "tags": ["sdn", "telemetry"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "sFlow Packets Received",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(sflow_packets_total[1m])",
            "legendFormat": "sFlow packets/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "gNMI Updates Received",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(gnmi_updates_total[1m])",
            "legendFormat": "gNMI updates/sec"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Total sFlow Packets",
        "type": "singlestat",
        "targets": [
          {
            "expr": "sflow_packets_total"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Total gNMI Updates",
        "type": "singlestat",
        "targets": [
          {
            "expr": "gnmi_updates_total"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8}
      }
    ],
    "schemaVersion": 16,
    "version": 1,
    "refresh": "5s"
  }
}

